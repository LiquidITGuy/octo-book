# Clés VAPID pour les notifications push
# Générez vos propres clés avec: node generate-vapid-keys.js
VAPID_PUBLIC_KEY=your_vapid_public_key_here
VAPID_PRIVATE_KEY=your_vapid_private_key_here
VAPID_EMAIL=mailto:your-email@example.com

# Configuration serveur
PORT=3200
NODE_ENV=development

# Sécurité notifications
NOTIFICATION_PASSWORD=your_secure_password_here

# Base de données PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=octobooks
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DATABASE_URL=postgresql://your_db_user:your_db_password@localhost:5432/octobooks
